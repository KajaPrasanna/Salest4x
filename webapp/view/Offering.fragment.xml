<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog 
        id="Selectdialog5"
		title="Customer"
		search=".onSearch5"
		confirm=".onOfferingValueHelpDialogClose"
		cancel=".onValueHelpDialogClose"
		showClearButton="true"
		items="{OfferingModel>/}">
		<StandardListItem
			title="{OfferingModel>OfferingId}"
			description="{OfferingModel>OfferingName}"
			iconDensityAware="false"
			iconInset="false"/>
	</SelectDialog>
</core:FragmentDefinition>